Host *
    ServerAliveInterval 60

# Bytemark

Host *.bytemark.co.uk Host *.dh *.sh *.ns *.vm *.vmh *.vmadmin *.managed *.yrk *.man
	User charles

# York

Host ssh.york.ac.uk
    PubKeyAuthentication yes
    IdentityFile ~/.ssh/id_rsa
    User cp872

Host *.york.ac.uk !ssh.york.ac.uk
    ProxyCommand ssh ssh.york.ac.uk -W %h:%p

Host csteach0
    HostName csteach0.york.ac.uk
    ProxyCommand ssh ssh.york.ac.uk -W %h:%p
    User cp872

# Gateway
Host csteach1
    HostName csteach1.york.ac.uk
    User cp872

